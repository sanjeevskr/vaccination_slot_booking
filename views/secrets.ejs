<%- include('partials/header') %>


<h2>STUDENTS</h2>

<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <table class="table table-bordered table-hover dt-responsive tablemobile">

        <thead>
          <tr>
            <th>Roll no.</th>
            <th>Name</th>
            <th>Register Number</th>
            <th>Time</th>
            <th>Date</th>
          </tr>
        </thead>
        <tbody>
          <% if(currentItems.length===0){ %>
          <tr>
            <td>.... </td>
            <td>.... </td>
            <td>.... </td>
            <td>.... </td>
            <td>.... </td>
          </tr>
          <%}else{%>

            <%for(var i=0;i<listItem.length;i++){ %>
              <%let n=0;k=0%>
              <%for(var j=0;j<currentItems.length;j++){%>
                <%if(currentItems[j].name.toLowerCase()===listItem[i].name.toLowerCase()){%>
                  <%n=1;%>
                  <%if(n===1){%>
                    <%k=j;%>
                  <%}%>
                <%}%>
              <%}%>
              <%if(n===1){%>
              <tr>
                <td class="highlight"><%= listItem[i].rollNumber %></td>
                <td class="highlight"><%= listItem[i].name%></td>
                <td class="highlight"><%= listItem[i].registerNumber %></td>
                <td class="highlight"><%= currentItems[k].time %></td>
                <td class="highlight"><%=date %></td>
              </tr>
              <%}else{%>
                <tr>
                  <td><%= listItem[i].rollNumber %></td>
                  <td><%= listItem[i].name%></td>
                  <td><%= listItem[i].registerNumber %></td>
                  <td><%= listItem[i].time %></td>
                  <td><%=date %></td>
                </tr>
              <%}%>
            <%}%>
          <%}%>


        </tbody>
      </table>
    </div>
  </div>
</div>

